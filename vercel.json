{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/images/(.*)", "dest": "/public/images/$1" },
      { "src": "/style.css", "dest": "/public/style.css" },
      { "src": "/validateform.js", "dest": "/public/validateform.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }